WHILE and FOR LOOP works in javascript

#code for WHILE LOOP

var k=99;

while(k>0){
    console.log(k+" bottles of beer on the wall"+k+" bottles of beer");
    console.log("Take one down and pass it around,"+(k-1)+" bottles of beer on the wall.");
    k--;
}

console.log("No more bottles of beer on the wall, no more bottles of beer.") 
console.log("Go to the store and buy some more, 99 bottles of beer on the wall." )


#code for FOR LOOP

for(var k=99;k>0;k--){
 console.log(k+" bottles of beer on the wall"+k+" bottles of beer");
    console.log("Take one down and pass it around,"+(k-1)+" bottles of beer on the wall.");
}
console.log("No more bottles of beer on the wall, no more bottles of beer.") 
console.log("Go to the store and buy some more, 99 bottles of beer on the wall." )
