#js 

import React from "react";
import ReactDOM from "react-dom";
import App from "./App";

ReactDOM.render(<App/>,
  document.getElementById("root")
);

#jsx-heading

import React from "react";

function Heading(){
  return <h1>My Favourite Foods</h1>;
}

export default Heading;

#jsx-list
import React from "react";

function List(){
  return  <ul>
  <li>Bacon</li>
  <li>Jamon</li>
  <li>Noodles</li>
</ul>
}

export default List;

#jsx-app
import React from "react";
import Heading from "./Heading";
import List from "./List";

function App(){
  return (
  <div>
  <Heading/>
  <List/>
</div>
  );
}

export default App;
