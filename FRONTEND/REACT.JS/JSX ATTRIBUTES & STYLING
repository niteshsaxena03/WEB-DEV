#applying css

.heading{
  color: red;
}

ul{
  color:blue;
}

img{
  height: 100px;
  width: 100px;
}

#JSX

import React from "react";
import ReactDOM from "react-dom";
const image="https://picsum.photos/200"
ReactDOM.render(
  <div>
    <h1 className="heading">My Favourite Foods</h1>
    <div>
    <img src={image}/>
    </div>
  </div>,
  document.getElementById("root")
);
